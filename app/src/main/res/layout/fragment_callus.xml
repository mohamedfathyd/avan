<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/transparent"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"


            android:layout_height="match_parent">
            <ScrollView
                android:layout_width="match_parent"

                android:layout_height="wrap_content">
                <RelativeLayout
                    android:paddingLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"

                    android:paddingRight="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/callus"
                        android:textStyle="bold"
                        android:textColor="@color/white"
                        android:layout_marginRight="20dp"
                        android:fontFamily="@font/taj"
                        android:layout_marginLeft="20dp"
                        android:layout_alignParentEnd="true"
                        android:textSize="18dp"
                        android:id="@+id/title"
                        android:background="@drawable/backshapeblue"
                        />

                    <ImageView
                        android:layout_width="180dp"
                        android:layout_centerHorizontal="true"
                        android:layout_below="@id/title"
                        android:layout_height="180dp"
                        android:background="@drawable/logored"
                        android:id="@+id/image"
                        />
                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/image"
                        android:orientation="vertical">


                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/a"
                            android:id="@+id/b"
                            android:layout_marginTop="5dp" >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/backshapeemptybluew"
                                android:layout_marginTop="15dp"
                                android:layout_marginLeft="10dp"
                                android:orientation="horizontal"
                                android:layout_marginRight="10dp"
                                android:weightSum="6"
                                >

                                <EditText
                                    android:layout_width="0dp"
                                    android:layout_gravity="center_vertical"
                                    android:paddingRight="5dp"
                                    android:paddingLeft="5dp"
                                    android:textColor="@color/black"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/taj"
                                    android:layout_weight="6"
                                    android:id="@+id/textInputEditTextname"
                                    android:inputType="text"
                                    android:background="@null"
                                    />
                            </LinearLayout>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="30dp"
                                android:layout_marginTop="0dp"
                                android:layout_marginRight="30dp"
                                android:background="#FCFCFC"
                                android:padding="5dp"
                                android:fontFamily="@font/taj"
                                android:text="@string/name"
                                android:textColor="@color/colorPrimaryDark"
                                android:textSize="15dp" />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp" >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/backshapeemptybluew"
                                android:layout_marginTop="15dp"
                                android:layout_marginLeft="10dp"
                                android:orientation="horizontal"
                                android:layout_marginRight="10dp"
                                android:weightSum="6"
                                >

                                <EditText
                                    android:layout_width="0dp"
                                    android:layout_gravity="center_vertical"
                                    android:paddingRight="5dp"
                                    android:paddingLeft="5dp"
                                    android:fontFamily="@font/taj"
                                    android:textColor="@color/black"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="6"
                                    android:id="@+id/textInputEditTextphonee"
                                    android:inputType="phone"
                                    android:background="@null"
                                    />
                            </LinearLayout>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="30dp"
                                android:layout_marginTop="0dp"
                                android:fontFamily="@font/taj"
                                android:layout_marginRight="30dp"
                                android:background="#FCFCFC"
                                android:padding="5dp"
                                android:text="@string/phone"
                                android:textColor="@color/colorPrimaryDark"
                                android:textSize="15dp" />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp" >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/backshapeemptybluew"
                                android:layout_marginTop="15dp"
                                android:layout_marginLeft="10dp"
                                android:orientation="horizontal"
                                android:layout_marginRight="10dp"
                                android:weightSum="6"
                                >

                                <EditText
                                    android:layout_width="0dp"
                                    android:layout_gravity="center_vertical"
                                    android:paddingRight="5dp"
                                    android:paddingLeft="5dp"
                                    android:fontFamily="@font/taj"
                                    android:textColor="@color/black"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="6"
                                    android:id="@+id/textInputEditTextphone"
                                    android:inputType="textEmailAddress"
                                    android:background="@null"
                                    />
                            </LinearLayout>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="30dp"
                                android:layout_marginTop="0dp"
                                android:fontFamily="@font/taj"
                                android:layout_marginRight="30dp"
                                android:background="#FCFCFC"
                                android:padding="5dp"
                                android:text="@string/address"
                                android:textColor="@color/colorPrimaryDark"
                                android:textSize="15dp" />
                        </RelativeLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp" >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/backshapeemptybluew"
                                android:layout_marginTop="15dp"
                                android:layout_marginLeft="10dp"
                                android:orientation="horizontal"
                                android:layout_marginRight="10dp"
                                android:weightSum="6"
                                >

                                <EditText
                                    android:layout_width="0dp"
                                    android:layout_gravity="center_vertical"
                                    android:paddingRight="5dp"
                                    android:paddingLeft="5dp"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/taj"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="6"
                                    android:id="@+id/textInputEditTextsubject"
                                    android:inputType="text"
                                    android:background="@null"
                                    />
                            </LinearLayout>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="30dp"
                                android:layout_marginTop="0dp"
                                android:layout_marginRight="30dp"
                                android:background="#FCFCFC"
                                android:padding="5dp"
                                android:text="@string/Subject"
                                android:fontFamily="@font/taj"
                                android:textColor="@color/colorPrimaryDark"
                                android:textSize="15dp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp" >
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/backshapeemptybluew"
                                android:layout_marginTop="15dp"
                                android:layout_marginLeft="10dp"
                                android:orientation="horizontal"
                                android:layout_marginRight="10dp"
                                android:weightSum="6"
                                >

                                <EditText
                                    android:layout_width="0dp"
                                    android:layout_gravity="center_vertical"
                                    android:paddingRight="5dp"
                                    android:paddingLeft="5dp"
                                    android:fontFamily="@font/taj"
                                    android:textColor="@color/black"
                                    android:layout_height="95dp"
                                    android:layout_weight="6"
                                    android:id="@+id/textInputEditTextmessage"
                                    android:inputType="text"
                                    android:background="@null"
                                    />
                            </LinearLayout>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="30dp"
                                android:layout_marginTop="0dp"
                                android:layout_marginRight="30dp"
                                android:background="#FCFCFC"
                                android:padding="5dp"
                                android:fontFamily="@font/taj"
                                android:text="@string/message"
                                android:textColor="@color/colorPrimaryDark"
                                android:textSize="15dp" />
                        </RelativeLayout>

                        <androidx.appcompat.widget.AppCompatButton
                            android:id="@+id/appCompatButtonRegister"
                            android:background="@drawable/backshaperedg"
                            android:layout_width="200dp"
                            android:layout_gravity="center"
                            android:layout_height="50dp"
                            android:layout_marginTop="25dp"
                            android:layout_marginBottom="25dp"
                            android:textAllCaps="false"
                            android:textStyle="bold"
                            android:fontFamily="@font/taj"
                            android:textColor="@color/white"
                            android:text="@string/send" />


                    </androidx.appcompat.widget.LinearLayoutCompat>

                </RelativeLayout>
            </ScrollView>

        </androidx.core.widget.NestedScrollView>
    </RelativeLayout>
</FrameLayout>